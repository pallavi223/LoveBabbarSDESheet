Input: prices = [7,1,5,3,6,4]
Output: 5


class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int minProfit = INT_MAX;
        int maxProfit = INT_MIN;
        for(int i=0;i<prices.size();i++){
            minProfit = min(minProfit,prices[i]);
            maxProfit = max(maxProfit,prices[i]-minProfit);
        }
        return maxProfit;
    }
};
